{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "kenyaemr": "^19.0.0"
  },
  "pages": [
    {
      "component": "about",
      "route": "about",
      "online": true,
      "offline": true
    }
